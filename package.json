{
  "name": "ExpressReactStartKit",
  "version": "3.5.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/caofb/ExpressReactStartKit.git"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "test": "mocha --reporter spec --timeout 5000"
  },
  "betterScripts": {
    "start-dev": {
      "command": "node ./app.js",
      "env": {
        "NODE_ENV": "dev",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node ./webapp/devserver.js",
      "env": {
          "NODE_ENV": "dev"
      }
    }
  },
  "dependencies": {
    "async": "^1.5.2",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "connect-mongo": "^1.1.0",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "express-validator": "^2.19.2",
    "jade": "^1.11.0",
    "lodash": "^4.6.1",
    "lusca": "^1.3.0",
    "mongoose": "^4.4.6",
    "morgan": "^1.7.0",
    "multer": "^1.1.0",
    "nodemailer": "^2.3.0",
    "passport": "0.3.2",
    "passport-local": "^1.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "redux": "^3.3.1",
    "redux-simple-router": "^2.0.4",
    "redux-thunk": "^2.0.1",
    "request": "^2.69.0",
    "validator": "^5.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "better-npm-run": "0.0.8",
    "bower-webpack-plugin": "^0.1.9",
    "chai": "^3.5.0",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.5.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "lodash": "^4.6.1",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "null-loader": "^0.1.1",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "style-loader": "^0.13.1",
    "supertest": "^1.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "license": "MIT"
}
